#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Internship/new_hire_ai_prototype/Mod3/node_modules/.pnpm/svg-path-bbox@0.0.49/node_modules/svg-path-bbox/src/node_modules:/mnt/e/Internship/new_hire_ai_prototype/Mod3/node_modules/.pnpm/svg-path-bbox@0.0.49/node_modules/svg-path-bbox/node_modules:/mnt/e/Internship/new_hire_ai_prototype/Mod3/node_modules/.pnpm/svg-path-bbox@0.0.49/node_modules:/mnt/e/Internship/new_hire_ai_prototype/Mod3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Internship/new_hire_ai_prototype/Mod3/node_modules/.pnpm/svg-path-bbox@0.0.49/node_modules/svg-path-bbox/src/node_modules:/mnt/e/Internship/new_hire_ai_prototype/Mod3/node_modules/.pnpm/svg-path-bbox@0.0.49/node_modules/svg-path-bbox/node_modules:/mnt/e/Internship/new_hire_ai_prototype/Mod3/node_modules/.pnpm/svg-path-bbox@0.0.49/node_modules:/mnt/e/Internship/new_hire_ai_prototype/Mod3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../svg-path-bbox@0.0.49/node_modules/svg-path-bbox/src/cli.js" "$@"
else
  exec node  "$basedir/../../../../../svg-path-bbox@0.0.49/node_modules/svg-path-bbox/src/cli.js" "$@"
fi
